{
  "name": "mollie/integration-core",
  "description": "Mollie integrations core library",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=5.3",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Mollie\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mollie\\Tests\\": "tests",
      "Mollie\\Console\\": "console"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35",
    "codacy/coverage": "dev-master",
    "symfony/console": "^5.1"
  }
}
